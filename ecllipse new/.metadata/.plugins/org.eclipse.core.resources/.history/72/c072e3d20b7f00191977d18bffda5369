package JavaPrograms;

public class PatternProgSquare 
{

	public static void main(String[] args) 
	{
		for(int r = 1; r<=3; r++)
		{
			for(int c=1; c<=3 ; c++)
			{
				if(r>1&&r<3 && c>1&&c<3)
				{
					System.out.println("  ");
				}
				else
				{
					System.out.print("*");
				}
			}
			System.out.println();
		}
	}

}
