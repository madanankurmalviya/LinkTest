package JavaPrograms;

public class BubbleSortOne 
{
	
	public static void main(String[] args) 
	{
		int[] a ={22,1,78,108,5,2,80,50,20};
		System.out.println("The length of array is : "+a.length+" "+a[1]+" "+a[3]);
		for(int r=1;r<=a.length;r++)
		{
			int temp=0;
			for(int c=1;c<=a.length-r;c++)
			{
				if(a[c]>a[c+1])
				{
					temp=a[c];
					a[c]=a[c+1];
					a[c+1]=temp;
				}
				
			}
			System.out.println("Here the sorting done");
		}
	}

}
